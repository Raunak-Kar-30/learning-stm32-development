#ifndef USART_INTERRUPT_H_
#define USART_INTERRUPT_H_

// Includes
#include "stm32f4xx.h"
#include "utils.h"

// Defines
#ifndef SYS_CLOCK_FREQ
#define SYS_CLOCK_FREQ	16000000
#endif

#define USART2_CLK_EN		(1UL << 17)
#define USART2_RXNEIE		(1UL << 5)
#define USART2_EN			(1UL << 0)

// Function declarations (public)
void usart2_init(void);


#endif /* USART_INTERRUPT_H_ */
