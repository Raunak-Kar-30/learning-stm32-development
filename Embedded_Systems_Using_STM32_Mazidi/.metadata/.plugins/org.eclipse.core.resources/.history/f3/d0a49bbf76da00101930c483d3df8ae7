#ifndef INTERRUPTS_H_
#define INTERRUPTS_H_

// Includes
#include "stm32f4xx.h"
#include "utils.h"
#include <stdint.h>

// Defines
#define GPIOA_CLK_EN		(1UL << 0)
#define GPIOC_CLK_EN		(1UL << 2)
#define SYSCFG_CLK_EN		(1UL << 14)
#define EXTI10_UNMASK		(1UL << 10)
#define EXTI10_FTSR_EN		(1UL << 10)
#define EXTI12_UNMASK		(1UL << 12)
#define EXTI12_FTSR_EN		(1UL << 12)

// Function declarations (public)
blink_led_on_button_press();

#endif /* INTERRUPTS_H_ */
